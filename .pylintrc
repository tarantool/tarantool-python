[MAIN]
# Enable backward compatibility with pylint for pylint down to Python 3.6 one
ignore=bad-option-value

[BASIC]

# Good variable names which should always be accepted, separated by a comma
good-names=i,j,k,ex,Run,_,ok,t,tz

[FORMAT]
# Allow links in docstings, allow tables
ignore-long-lines=^(?:\s*(# )?(?:\.\.\s.+?:)?\s*<?https?://\S+>?)|(\s\+.+\+)|(\s\|.+\|)$
